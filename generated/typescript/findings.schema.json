      "description": "Code context for a finding.\n\nProvides location and snippet information for findings related to specific code.",
      "properties": {
        "file": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "File path where the finding was detected",
          "title": "File"
        },
        "line": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Line number where the finding was detected",
          "title": "Line"
        },
        "column": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Column number where the finding was detected",
          "title": "Column"
        },
        "snippet": {
          "anyOf": [
            {
              "maxLength": 1000,
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Code snippet related to the finding (max 1000 chars)",
          "title": "Snippet"
        }
      },
      "title": "FindingCodeContext",
      "type": "object"
    },
    "FindingUserInput": {
      "description": "User input request for a finding.\n\nDefines the question and input format when a finding requires user decision.",
      "properties": {
        "question": {
          "description": "Question to present to the user",
          "title": "Question",
          "type": "string"
        },
        "input_type": {
          "default": "text",
          "description": "Type of input expected: 'text' or 'choice'",
          "title": "Input Type",
          "type": "string"
        },
        "options": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Options for choice-type input",
          "title": "Options"
        }
      },
      "required": [
        "question"
      ],
      "title": "FindingUserInput",
      "type": "object"
    },
