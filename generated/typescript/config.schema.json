{
  "schemas": {
    "ContextAutoInclude": {
      "description": "Rules for automatically including a context in AI tasks.\n\nWhen an AI task is created, the runner evaluates these rules to determine\nwhich contexts should be automatically included. Multiple rules are OR'd\ntogether (any match triggers inclusion).",
      "properties": {
        "taskMentions": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Keywords in task prompt that trigger inclusion (case-insensitive)",
          "title": "Taskmentions"
        },
        "actionTypes": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Action types in loaded config that trigger inclusion (e.g., 'CLICK', 'FIND')",
          "title": "Actiontypes"
        },
        "errorPatterns": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Regex patterns in recent logs that trigger inclusion",
          "title": "Errorpatterns"
        },
        "filePatterns": {
          "anyOf": [
            {
              "items": {
                "type": "string"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Glob patterns for files being worked on (e.g., '*.rs', 'src/api/**')",
          "title": "Filepatterns"
        }
      },
      "title": "ContextAutoInclude",
      "type": "object"
    },
